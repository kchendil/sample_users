dn: cn=schema
changetype: modify
add: attributeTypes
attributeTypes: ( sampleDeviceType-oid NAME 'sampleDeviceType' 
  DESC 'Device type used for EPM sample' 
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE)

dn: cn=schema
changetype: modify
add: attributeTypes
attributeTypes: ( sampleDeviceModel-oid NAME 'sampleDeviceModel' 
  DESC 'Device type used for EPM sample data' 
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE)

dn: cn=schema
changetype: modify
add: objectclasses
objectClasses: ( sampleDeviceAux-oid NAME 'sampleDeviceAux' DESC 'Extended Device type'
 AUXILIARY MAY ( sampleDeviceType $ sampleDeviceModel ) X-NDS_NOT_CONTAINER '1' )

dn: cn=schema
changetype: modify
add: attributeTypes
attributeTypes: ( sampleDeviceDN-oid NAME 'sampleDeviceDN' 
  DESC 'Device DN'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12)

dn: cn=schema
changetype: modify
add: objectclasses
objectClasses: ( sampleUserDeviceAux-oid NAME 'sampleUserDeviceAux' DESC 'Device list for user' 
 AUXILIARY MAY ( sampleDeviceDN) X-NDS_NOT_CONTAINER '1' )

